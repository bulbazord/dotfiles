set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'helino/vim-nasm'

call vundle#end()
filetype plugin indent on

syntax on
set encoding=utf-8
set number
set showcmd
set ruler
set backspace=2

" Only 80 characters per line, please
set colorcolumn=81
colorscheme base16-monokai
